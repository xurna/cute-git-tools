{
    "name": "cute-git-tools",
    "version": "1.1.0",
    "description": "git tools",
    "main": "./bin/index.js",
    "bin": {
        "git-auto-commit": "bin/index.js"
    },
    "scripts": {
        "merge": "git-auto-commit --config=./git-auto-commit.config.json",
        "merge:cli": "git-auto-commit -m='feat: 优化' --target=master --sit=wx_dev,h5_dev"
    },
    "author": "xiaonayang",
    "keywords": [
        "git-auto-commit",
        "git-tools"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/xurna/cute-git-tools/issues"
    },
    "homepage": "https://github.com/xurna/cute-git-tools#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/xurna/cute-git-tools.git"
    },
    "lint-staged": {
        "*{.js,ts,vue}": "eslint . --fix --max-warnings 0"
    },
    "dependencies": {
        "chalk": "^4.1.2",
        "inquirer": "^8.2.0",
        "path": "0.12.7",
        "shelljs": "0.8.4",
        "yargs": "17.0.1"
    },
    "devDependencies": {
        "eslint": "7.32.0",
        "eslint-config-standard": "16.0.3",
        "eslint-plugin-import": "2.24.2",
        "eslint-plugin-node": "11.1.0",
        "eslint-plugin-promise": "5.1.0",
        "husky": "7.0.2",
        "lint-staged": "11.1.2"
    }
}
